{
    "name": "sdpi-components",
    "description": "Web components for your Stream Deck plugin",
    "main": "./dist/sdpi-components.js",
    "scripts": {
        "dist": "webpack",
        "format": "prettier --config .prettierrc './src/**/*.ts' --write",
        "lint": "eslint . --ext .ts",
        "postinstall": "node ./scripts/install-example.js",
        "start": "webpack -w --env local & npm run start:example",
        "start:example": "onchange './example/**/*.*' -- npm run postinstall"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GeekyEggo/sdpi-components.git"
    },
    "keywords": [
        "stream-deck",
        "stream",
        "deck",
        "property-inspector"
    ],
    "author": "GeekyEggo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/GeekyEggo/sdpi-components/issues"
    },
    "homepage": "https://sdpi-components.dev",
    "devDependencies": {
        "@lit-labs/task": "^1.1.1",
        "@trivago/prettier-plugin-sort-imports": "^3.2.0",
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "fs-extra": "^10.0.1",
        "lit": "^2.2.1",
        "onchange": "^7.1.0",
        "prettier": "^2.6.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.2",
        "webpack": "^5.64.4",
        "webpack-cli": "^4.9.1"
    }
}
