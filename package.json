{
    "name": "sdpi",
    "version": "1.0.0",
    "description": "Stream Deck Property Inspector web components",
    "main": "src/index.ts",
    "scripts": {
        "build": "webpack",
        "watch": "webpack -w --env local & npm run watch:example",
        "format": "prettier --config .prettierrc './src/**/*.ts' --write",
        "lint": "eslint . --ext .ts",
        "install:example": "onchange './example/**/*.*' -- npm run install:example",
        "watch:example": "node ./scripts/install-example.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GeekyEggo/sdpi.git"
    },
    "keywords": [
        "stream-deck",
        "stream",
        "deck",
        "property-inspector"
    ],
    "author": "GeekyEggo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/GeekyEggo/sdpi/issues"
    },
    "homepage": "https://github.com/GeekyEggo/sdpi#readme",
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.16.0",
        "@typescript-eslint/parser": "^5.16.0",
        "css-loader": "^6.5.1",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "fs-extra": "^10.0.1",
        "onchange": "^7.1.0",
        "prettier": "^2.6.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.2",
        "webpack": "^5.64.4",
        "webpack-cli": "^4.9.1"
    }
}
